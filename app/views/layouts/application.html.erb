<!DOCTYPE html>
<html>
<head>
  <title>Twenty48Rails</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container" >
      <% if !flash.empty? %>
        <div class="flash">
          <% flash.each do |key, value| %><%= value %>
          <% end %>
        </div>
      <% end %>
      <%= render 'navbar' %>
      <%= yield %>
    </div>
</body>
</html>
