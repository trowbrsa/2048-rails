<h1>Saved Games</h1>
<h4>Click on a game to resume</h4>
<table class="table table-hover">
  <tbody>
    <tr>
      <th>Game</th>
      <th>Current Score</th>
    </tr>
    <% count = 0 %>
    <% @current_user.games.each do |game| %>
    <tr>
      <td><%= count+= 1 %></td>
      <td><%= game.current_score %></td>
      <td><button class="resume" value=<game.id>Resume Game</game.id></td>
    <% end %>
    </tr>
  </tbody>
</table>
