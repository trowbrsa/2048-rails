<h1>Saved Games</h1>
<h4>Click on a game to resume</h4>  
<table class="table table-hover">
  <tbody>
    <tr>
      <th>Game</th>
      <th>Current Score</th>
    </tr>
    <% @current_user.games.each do |game| %>
      <% count = 1 %>
    <tr>
      <td><%= count %></td>
      <td><%= game.current_score %></td>
      <% count += 1 %>
    <% end %>
    </tr>
  </tbody>
</table>
